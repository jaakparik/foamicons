{
  "name": "foamicons",
  "version": "0.2.1",
  "description": "Foam icon library for React",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "generate": "tsx scripts/build-icons.ts",
    "build": "npm run generate && tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "icons",
    "react",
    "svg",
    "foam",
    "foamicons",
    "tailwind",
    "shadcn"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
